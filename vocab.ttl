@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vs: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix o: <http://omeka.org/s/vocabs/o#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix ma: <http://www.w3.org/ns/ma-ont#>.
@prefix num: <https://jardindesconnaissances.univ-paris8.fr/onto/num#> .

<https://jardindesconnaissances.univ-paris8.fr/onto/num#> a owl:Ontology ;
    dcterms:title "Le numérique dans l'éducation à l'Université Paris 8" .

num:OutilNumerique a rdfs:Class ;
    rdfs:label "OutilNumerique" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

num:Concepteur a rdfs:Class ;
    rdfs:label "Concepteur" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

num:Utilisateur a rdfs:Class ;
    rdfs:label "Utilisateur" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

num:Personne a rdfs:Class ;
    rdfs:label "Personne" ;
    rdfs:domain o:Resource ;
    rdfs: subClassOf : Utilisateur ;
    vs:term_status "experimental" .

num:Etudiant a rdfs:Class ;
    rdfs:label "Etudiant" ;
    rdfs:domain o:Resource ;
    rdfs: subClass of : Personne
    vs:term_status "experimental" .

num:Enseignant a rdfs:Class ;
    rdfs:label "Enseignant" ;
    rdfs:domain o:Resource ;
    rdfs: subClass of : Personne
    vs:term_status "experimental" .

num:ServicePedagogique a rdfs:Class ;
    rdfs:label "ServicePedagogique" ;
    rdfs:domain o:Resource ;
    rdfs: subClassOf : Utilisateur ;
    vs:term_status "experimental" .

num:Cours a rdfs:Class ;
    rdfs:label "Cours" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

num:hasUtilisateur a rdf:Property ;
    rdfs:label "à comme utilisateur" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

num:hasConcepteur a rdf:Property ;
    rdfs:label "à comme concepteur" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

num:hasCours a rdf:Property ;
    rdfs:label "à comme cours" ;
    rdfs:domain o:Resource ;
    vs:term_status "experimental" .

num:hasId as rdf: Property
    rdfs:label "a comme identifiant";
    rdfs:domain :Utilisateur, :Personne, :Etudiant, :Enseignant, :ServicePedagogique, :Concepteur, :Cours, :Batiment ;
    vs:term_status "experimental" .

num:hasNom as rdf: Property
    rdfs:label "a comme nom";
    rdfs:domain o:Personne, :Concepteur, :Batiment ;
    vs:term_status "experimental" .

num:hasPrenom as rdf: Property
    rdfs:label "a comme prénom";
    rdfs:domain :Personne ;
    vs:term_status "experimental" .

num:hasMail a rdf:Property ;
    rdfs:domain :Personne, :ServicePedagogique, :Concepteur ;
    rdfs:label "a comme Adresse mail" ;
    vs:term_status "experimental" .

num:hasNum_tel a rdf:Property ;
    rdfs:domain :Personne, :ServicePedagogique ;
    rdfs:label "a comme Numéro de téléphone" ;
    vs:term_status "experimental" .

num:hasNum_etu a rdf:Property ;
    rdfs:domain :Etudiant ;
    rdfs:label "a comme Numéro étudiant" ;
    vs:term_status "experimental" .

num:hasFormation a rdf:Property ;
    rdfs:domain :Enseignant ;
    rdfs:label "Formation" ;
    vs:term_status "experimental" .

num:hasId_cours a rdf:Property ;
    rdfs:domain :Enseignant ;
    rdfs:label "a comme cours" ;
    vs:term_status "experimental" .

num:hasNom_service a rdf:Property ;
    rdfs:domain :ServicePedagogique ;
    rdfs:label "a comme Nom de service pédagogique" ;
    vs:term_status "experimental" .

num:hasId_batiment a rdf:Property ;
    rdfs:domain :ServicePedagogique, :Concepteur ;
    rdfs:label "a comme bâtiment" ;
    vs:term_status "experimental" .

num:hasNom_cours a rdf:Property ;
    rdfs:domain :Cours ;
    rdfs:label "Nom du cours" ;
    vs:term_status "experimental" .

num:hasNum a rdf:Property ;
    rdfs:domain :Bureau ;
    rdfs:label "a comme Numéro de bureau" ;
    vs:term_status "experimental" .

num:hasDescription a rdf:Property ;
    rdfs:domain :OutilNumerique ;
    rdfs:label "a comme Description" ;
    vs:term_status "experimental" .
